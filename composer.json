{
    "name": "enqueue/psr-queue",
    "type": "library",
    "description": "Enqueue Message Service (based on Java Message Service)",
    "keywords": ["messaging", "queue", "java message service", "jms"],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:php-enqueue/test.git"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5",
        "enqueue/test": "^0.2"
    },
    "autoload": {
        "psr-4": { "Enqueue\\Psr\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
